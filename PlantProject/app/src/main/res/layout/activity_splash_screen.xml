<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccent"
    tools:context=".SplashScreen">

    <ImageView
        android:id="@+id/connect_image"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:contentDescription="@string/connect"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4"
        app:srcCompat="@drawable/plant" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lilita_one"
        android:text="@string/plantiful"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="60sp"
        app:layout_constraintBottom_toTopOf="@+id/connect_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lilita_one"
        android:gravity="end"
        android:text="@string/if_you_have_a_garden_and_a_library_nyou_have_everything_you_need_n_marcus_tullius_cicero"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/connect_image"
        app:layout_constraintVertical_bias="0.35" />

    <View
        android:id="@+id/divider6"
        android:layout_width="6dp"
        android:layout_height="54dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toStartOf="@+id/connect_image"
        app:layout_constraintTop_toTopOf="@+id/connect_image" />

    <View
        android:id="@+id/divider7"
        android:layout_width="6dp"
        android:layout_height="54dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="@+id/connect_image"
        app:layout_constraintEnd_toStartOf="@+id/connect_image" />

    <View
        android:id="@+id/divider8"
        android:layout_width="6dp"
        android:layout_height="54dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="@+id/connect_image"
        app:layout_constraintStart_toEndOf="@+id/connect_image" />

    <View
        android:id="@+id/divider9"
        android:layout_width="6dp"
        android:layout_height="54dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintStart_toEndOf="@+id/connect_image"
        app:layout_constraintTop_toTopOf="@+id/connect_image" />

    <View
        android:id="@+id/divider10"
        android:layout_width="60dp"
        android:layout_height="6dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintBottom_toTopOf="@+id/connect_image"
        app:layout_constraintStart_toStartOf="@+id/divider6" />

    <View
        android:id="@+id/divider11"
        android:layout_width="60dp"
        android:layout_height="6dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintBottom_toTopOf="@+id/connect_image"
        app:layout_constraintEnd_toEndOf="@+id/divider9" />

    <View
        android:id="@+id/divider12"
        android:layout_width="60dp"
        android:layout_height="6dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="@+id/divider9"
        app:layout_constraintTop_toBottomOf="@+id/connect_image" />

    <View
        android:id="@+id/divider13"
        android:layout_width="60dp"
        android:layout_height="6dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintStart_toStartOf="@+id/divider7"
        app:layout_constraintTop_toBottomOf="@+id/connect_image" />
</androidx.constraintlayout.widget.ConstraintLayout>